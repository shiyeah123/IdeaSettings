<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Change Signature...">
        <Item>
          <IdeaActionID>ChangeSignature</IdeaActionID>
          <ShortCut>'Ctrl+F6'</ShortCut>
          <Description>Change Signature...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy...">
        <Item>
          <IdeaActionID>CopyElement</IdeaActionID>
          <ShortCut>'F5'</ShortCut>
          <Description>Copy...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'Delete'</ShortCut>
          <Description>Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Generate...">
        <Item>
          <IdeaActionID>Generate</IdeaActionID>
          <ShortCut>'Alt+Insert'</ShortCut>
          <Description>Generate...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Last Edit Location">
        <Item>
          <IdeaActionID>JumpToLastChange</IdeaActionID>
          <ShortCut>'Ctrl+Shift+Backspace'</ShortCut>
          <Description>Last Edit Location</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Move Line Down">
        <Item>
          <IdeaActionID>MoveLineDown</IdeaActionID>
          <ShortCut>'Alt+Shift+向下箭头'</ShortCut>
          <Description>Move Line Down</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste from History...">
        <Item>
          <IdeaActionID>PasteMultiple</IdeaActionID>
          <ShortCut>'Ctrl+Shift+V' or 'Ctrl+Shift+Insert'</ShortCut>
          <Description>Paste from History...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Scratch File">
        <Item>
          <IdeaActionID>NewScratchFile</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+Insert'</ShortCut>
          <Description>Scratch File</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Type Declaration">
        <Item>
          <IdeaActionID>GotoTypeDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+Shift+B' or 'Ctrl+Shift+Button1 Click'</ShortCut>
          <Description>Type Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Variable...">
        <Item>
          <IdeaActionID>IntroduceVariable</IdeaActionID>
          <ShortCut>'Ctrl+Alt+V'</ShortCut>
          <Description>Variable...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>